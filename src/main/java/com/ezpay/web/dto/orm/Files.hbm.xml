<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping 
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ezpay.web.dto.Files" table="tb_files">
		<id name="id" column="id" type="integer">
			<generator class="native" />
		</id>
		<property name="filename" column="filename" type="string" length="100"/>
		<property name="notes" column="notes" type="string" length="100"/>
		<property name="type" column="type" type="string" length="40"/>
		<property name="content" column="content" type="binary" />
	</class>
</hibernate-mapping>